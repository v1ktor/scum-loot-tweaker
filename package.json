{
  "name": "scum-loot-tweaker",
  "private": true,
  "version": "1.0.0",
  "description": "Web app to tweak loot spawn in SCUM game",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "node dist/index.js",
    "dev:nodemon": "nodemon --watch 'src/**/*.ts' --exec ts-node src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/v1ktor/scum-loot-tweaker.git"
  },
  "author": "Viktor Kalinin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/v1ktor/scum-loot-tweaker/issues"
  },
  "homepage": "https://v1ktor.github.io/scum-loot-tweaker",
  "dependencies": {
    "gh-pages": "^6.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.3",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "vite": "^5.0.8"
  }
}
